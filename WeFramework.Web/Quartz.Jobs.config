<?xml version="1.0" encoding="UTF-8"?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData">
  <processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>
  <schedule>
    <job>
      <name>jobName1</name>
      <group>jobGroup1</group>
      <description>jobDesciption1</description>
      <job-type>WeFramework.Web.Scheduler.SampleJob1,WeFramework.Web</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>triggerName1</name>
        <group>triggerGroup1</group>
        <job-name>jobName1</job-name>
        <job-group>jobGroup1</job-group>
        <cron-expression>2,10 * * * * ?</cron-expression>
      </cron>
    </trigger>
    <job>
      <name>jobName2</name>
      <group>jobGroup2</group>
      <description>jobDesciption3</description>
      <job-type>WeFramework.Web.Scheduler.SampleJob2,WeFramework.Web</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <simple>
        <name>triggerName2</name>
        <group>triggerGroup2</group>
        <description>SimpleTriggerDescription</description>
        <job-name>jobName2</job-name>
        <job-group>jobGroup2</job-group>
        <repeat-count>-1</repeat-count>
        <repeat-interval>20000</repeat-interval>
      </simple>
    </trigger>
    <job>
      <name>weChatAlertJob</name>
      <group>weChatGroup</group>
      <description>Sensor abnormal when wechat alarm</description>
      <job-type>WeFramework.Web.Scheduler.WeChatAlertJob,WeFramework.Web</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>weChatAlertJob</name>
        <group>weChatGroup</group>
        <job-name>weChatAlertJob</job-name>
        <job-group>weChatGroup</job-group>
        <cron-expression>0,20,40 * * * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>
